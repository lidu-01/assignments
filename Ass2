#include <iostream>
#include <string>
using namespace std;

int main() {
    string itemName;
    int quantity;
    double floatCost;
    double totalCost;

    cout << "Enter item name: ";
    cin >> itemName;

    cout << "Enter quantity: ";
    cin >> quantity;

    cout << "Enter cost per item: ";
    cin >> floatCost;

    totalCost = quantity * floatCost;
    cout << "totalCost " << itemName << ": $" << (quantity * totalCost) << endl;

    return 0;
}